    "express": "^4.16.4",
    "husky": "^2.2.0",
    "nyc": "^14.1.0",
    "sqlite3": "^4.0.6",
    "swagger-ui-express": "^4.0.2",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.7.0",
